# HTCondor submit file
universe = docker
docker_image = "krisrs1128/scpy:2024110412"
executable = run.sh
request_memory = 12GB
request_disk = 24GB
request_gpus = 1
+WantGPULab = true
+WantFlocking = true
+WantGliding = true
+GPUJobLength = "short" 
+is_resumable = true
log = run-$(Process).log
arguments = $(Process)
error = run-$(Process).err
output = run-$(Process).out
queue